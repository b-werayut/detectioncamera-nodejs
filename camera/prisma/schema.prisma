generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model TmstCameraDetectionLogs {
  id             Int     @id @default(autoincrement())
  foldername     String?
  linesendstatus String?
  picstatus      Int?    @default(0)
  vdostatus      Int?    @default(0)
  createdAt      String?
  updatedAt      String? @default("No Logs Update")
}

model TmstPowerDetectionLogs {
  id        Int      @id @default(autoincrement())
  point     String
  status    String
  value     Int
  timestamp String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model TmstLineUserIdCustomer {
  id         Int      @id @default(autoincrement())
  userID     String
  replyToken String
  quoteToken String
  timestamp  String
  createdAt  DateTime @default(now())
}

model electrical_data {
  id          Int       @id(map: "PK__electric__3213E83F2FC5526B") @default(autoincrement())
  device_code String?   @db.NVarChar(50)
  line        String?   @db.NVarChar(10)
  voltage     Decimal?  @db.Decimal(10, 2)
  currentel   Decimal?  @db.Decimal(10, 2)
  powerel     Decimal?  @db.Decimal(10, 2)
  energy      Decimal?  @db.Decimal(10, 3)
  frequency   Decimal?  @db.Decimal(5, 2)
  pf          Decimal?  @db.Decimal(5, 2)
  created_at  DateTime? @default(now(), map: "DF__electrica__creat__3C34F16F") @db.DateTime
}

model Cameras {
  CameraId       Int             @id(map: "PK__Cameras__F971E0C853A7FA4B") @default(autoincrement())
  CameraCode     String          @unique(map: "UQ__Cameras__27375FE60124A330") @db.NVarChar(50)
  CameraName     String          @db.NVarChar(255)
  Location       String?         @db.NVarChar(255)
  Status         String?         @default("Unknown", map: "DF__Cameras__Status__489AC854") @db.NVarChar(50)
  LastActiveTime DateTime?
  DateAdded      DateTime?       @default(now(), map: "DF__Cameras__DateAdd__498EEC8D")
  CameraStreams  CameraStreams[]
}

model CameraStreams {
  StreamId    Int      @id(map: "PK__CameraSt__07C87A925C36111C") @default(autoincrement())
  CameraId    Int
  StreamType  String   @db.NVarChar(50)
  StreamURL   String   @unique(map: "UQ__CameraSt__4E2724681264C193") @db.NVarChar(500)
  Description String?  @db.NVarChar(255)
  IsActive    Boolean? @default(true, map: "DF__CameraStr__IsAct__4D5F7D71")
  Cameras     Cameras  @relation(fields: [CameraId], references: [CameraId], onDelete: Cascade, onUpdate: NoAction, map: "FK_CameraStreams_Cameras")
}
